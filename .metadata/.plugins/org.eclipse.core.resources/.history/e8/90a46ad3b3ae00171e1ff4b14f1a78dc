package org.geetha.testng;

import java.util.*;

public class CodingExercise {
	
	private
		static Integer arrayA[] = {1,2,10,20};
		static Integer arrayB[] = {10,20,1,3,4,5};
		
	public void addMap(){
		HashMap<Integer, Integer> m1 = new HashMap<Integer, Integer>();
		
		
		for(int count : arrayA){
			if(m1.get(count))
			{
				m1.put(count, value)
			}
		}
		for(HashMap.Entry<Integer, Integer> entry : m1.entrySet(); int count : arrayA){
			if(entry.getKey() == arrayA[0]){
				int num = entry.getValue();
				entry.setValue(++num);
			} else {
				m1.put(arrayA[0], 1);				
			}
		}
 

		m1.put(arrayA[0], 1);
	
		for(HashMap.Entry<Integer, Integer> entry : m1.entrySet()){
			System.out.println(entry.getKey() + ":" + entry.getValue());
		}
	}
		
	
}