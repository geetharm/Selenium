package org.geetha.testng;

import java.util.*;

public class CodingExercise {
	
	private
		static Integer arrayA[] = {1,2,10,20};
		static Integer arrayB[] = {10,20,1,3,4,5};
		
	public void addMap(){
		HashMap<Integer, Integer> m1 = new HashMap<Integer, Integer>();
		
		
		for(int count : arrayA){
			if(m1.get(count))
			{
				m1.put(count, value)
			}
		}
		
		Iterator<HashMap.Entry<Integer, Integer>> entry = m1.entrySet().iterator();
		while(entry.hasNext()) {
			HashMap.Entry<Integer, Integer> pair = entry.next();
			
			for(int count : arrayA) {
				if(pair.getKey() == count){
					int num = pair.getValue();
					pair.setValue(++num);
				} else {
					m1.put(count, 1);
				}
		}

		while(entry.hasNext()) {
			System.out.println(entry.getKey() + ":" + entry.getValue());
		}
	}
		

}